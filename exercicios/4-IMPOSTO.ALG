Algoritmo "4-imposto"
// Data atual  : 10/01/2024
Var
// Seção de Declarações das variáveis
      preco, imposto: real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
      escreva ("qual o valor do produto?")
      leia (preco)
      imposto <- (preco *60)/ 100
      escreva ("o imposto sera de:", imposto)

Fimalgoritmo