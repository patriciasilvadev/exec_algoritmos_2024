Algoritmo "2-conversodolar"
// Data atual  : 10/01/2024
Var
// Seção de Declarações das variáveis
   reais, dolares: real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
      escreva ("Quantos reais eu tenho?")
      leia (reais)
      dolares <- reais/4.89
      escreva ("Entao tenho US$:", dolares)

Fimalgoritmo